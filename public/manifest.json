{
  "manifest_version": 3,
  "name": "FactLens",
  "version": "0.1.0",
  "description": "Quick fact-check analysis for tweets.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
  "16": "icon-16.png",
  "48": "icon-48.png",
  "128": "icon-128.png"
}
}
